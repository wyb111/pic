<script type="text/javascript">
	$(".pics ul").width($(".pics ul li").length*$(".pics ul li").width());
	$(".scroll-img ul").width($(".scroll-img ul li").length*$(".scroll-img ul li").width());

	for(var index=0;index < $(".pics ul li").length;index++){
		$("<a class='item "+(index==0?"active":"")+"'></a>").appendTo($(".pics .circle")).click(function() {
			/* Act on the event */
			$(".pics ul li").eq($(this).index()).show().siblings().hide();
			$(this).addClass('active').siblings().removeClass('active');
		});;
	}
</script>
